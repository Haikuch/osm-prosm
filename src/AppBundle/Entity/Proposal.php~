<?php

namespace AppBundle\Entity;

/**
 * Proposal
 */
class Proposal
{
    /**
     * @var integer
     */
    private $id;

    /**
     * @var string
     */
    private $name;

    /**
     * @var string
     */
    private $description;

    /**
     * @var string
     */
    private $content;

    /**
     * @var \DateTime
     */
    private $createdTime;

    /**
     * @var \DateTime
     */
    private $updatedTime;

    /**
     * @var \DateTime
     */
    private $draftStartTime;

    /**
     * @var \DateTime
     */
    private $rfcStartTime;

    /**
     * @var \DateTime
     */
    private $voteStartTime;

    /**
     * @var \DateTime
     */
    private $voteEndTime;

    /**
     * @var integer
     */
    private $createdUserId;

    /**
     * @var integer
     */
    private $views;

    /**
     * @var boolean
     */
    private $approved;

    /**
     * @var boolean
     */
    private $archived;

    /**
     * @var \AppBundle\Entity\Status
     */
    private $status;


    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set name
     *
     * @param string $name
     *
     * @return Proposal
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set description
     *
     * @param string $description
     *
     * @return Proposal
     */
    public function setDescription($description)
    {
        $this->description = $description;

        return $this;
    }

    /**
     * Get description
     *
     * @return string
     */
    public function getDescription()
    {
        return $this->description;
    }

    /**
     * Set content
     *
     * @param string $content
     *
     * @return Proposal
     */
    public function setContent($content)
    {
        $this->content = $content;

        return $this;
    }

    /**
     * Get content
     *
     * @return string
     */
    public function getContent()
    {
        return $this->content;
    }

    /**
     * Set createdTime
     *
     * @param \DateTime $createdTime
     *
     * @return Proposal
     */
    public function setCreatedTime($createdTime)
    {
        $this->createdTime = $createdTime;

        return $this;
    }

    /**
     * Get createdTime
     *
     * @return \DateTime
     */
    public function getCreatedTime()
    {
        return $this->createdTime;
    }

    /**
     * Set updatedTime
     *
     * @param \DateTime $updatedTime
     *
     * @return Proposal
     */
    public function setUpdatedTime($updatedTime)
    {
        $this->updatedTime = $updatedTime;

        return $this;
    }

    /**
     * Get updatedTime
     *
     * @return \DateTime
     */
    public function getUpdatedTime()
    {
        return $this->updatedTime;
    }

    /**
     * Set draftStartTime
     *
     * @param \DateTime $draftStartTime
     *
     * @return Proposal
     */
    public function setDraftStartTime($draftStartTime)
    {
        $this->draftStartTime = $draftStartTime;

        return $this;
    }

    /**
     * Get draftStartTime
     *
     * @return \DateTime
     */
    public function getDraftStartTime()
    {
        return $this->draftStartTime;
    }

    /**
     * Set rfcStartTime
     *
     * @param \DateTime $rfcStartTime
     *
     * @return Proposal
     */
    public function setRfcStartTime($rfcStartTime)
    {
        $this->rfcStartTime = $rfcStartTime;

        return $this;
    }

    /**
     * Get rfcStartTime
     *
     * @return \DateTime
     */
    public function getRfcStartTime()
    {
        return $this->rfcStartTime;
    }

    /**
     * Set voteStartTime
     *
     * @param \DateTime $voteStartTime
     *
     * @return Proposal
     */
    public function setVoteStartTime($voteStartTime)
    {
        $this->voteStartTime = $voteStartTime;

        return $this;
    }

    /**
     * Get voteStartTime
     *
     * @return \DateTime
     */
    public function getVoteStartTime()
    {
        return $this->voteStartTime;
    }

    /**
     * Set voteEndTime
     *
     * @param \DateTime $voteEndTime
     *
     * @return Proposal
     */
    public function setVoteEndTime($voteEndTime)
    {
        $this->voteEndTime = $voteEndTime;

        return $this;
    }

    /**
     * Get voteEndTime
     *
     * @return \DateTime
     */
    public function getVoteEndTime()
    {
        return $this->voteEndTime;
    }

    /**
     * Set createdUserId
     *
     * @param integer $createdUserId
     *
     * @return Proposal
     */
    public function setCreatedUserId($createdUserId)
    {
        $this->createdUserId = $createdUserId;

        return $this;
    }

    /**
     * Get createdUserId
     *
     * @return integer
     */
    public function getCreatedUserId()
    {
        return $this->createdUserId;
    }

    /**
     * Set views
     *
     * @param integer $views
     *
     * @return Proposal
     */
    public function setViews($views)
    {
        $this->views = $views;

        return $this;
    }

    /**
     * Get views
     *
     * @return integer
     */
    public function getViews()
    {
        return $this->views;
    }

    /**
     * Set approved
     *
     * @param boolean $approved
     *
     * @return Proposal
     */
    public function setApproved($approved)
    {
        $this->approved = $approved;

        return $this;
    }

    /**
     * Get approved
     *
     * @return boolean
     */
    public function getApproved()
    {
        return $this->approved;
    }

    /**
     * Set archived
     *
     * @param boolean $archived
     *
     * @return Proposal
     */
    public function setArchived($archived)
    {
        $this->archived = $archived;

        return $this;
    }

    /**
     * Get archived
     *
     * @return boolean
     */
    public function getArchived()
    {
        return $this->archived;
    }

    /**
     * Set status
     *
     * @param \AppBundle\Entity\Status $status
     *
     * @return Proposal
     */
    public function setStatus(\AppBundle\Entity\Status $status = null)
    {
        $this->status = $status;

        return $this;
    }

    /**
     * Get status
     *
     * @return \AppBundle\Entity\Status
     */
    public function getStatus()
    {
        return $this->status;
    }
    /**
     * @var boolean
     */
    private $creating;


    /**
     * Set creating
     *
     * @param boolean $creating
     *
     * @return Proposal
     */
    public function setCreating($creating)
    {
        $this->creating = $creating;

        return $this;
    }

    /**
     * Get creating
     *
     * @return boolean
     */
    public function getCreating()
    {
        return $this->creating;
    }
    /**
     * @var integer
     */
    private $authorId;

    /**
     * @var boolean
     */
    private $isApproved;

    /**
     * @var boolean
     */
    private $isArchived;

    /**
     * @var boolean
     */
    private $isCreating;


    /**
     * Set authorId
     *
     * @param integer $authorId
     *
     * @return Proposal
     */
    public function setAuthorId($authorId)
    {
        $this->authorId = $authorId;

        return $this;
    }

    /**
     * Get authorId
     *
     * @return integer
     */
    public function getAuthorId()
    {
        return $this->authorId;
    }

    /**
     * Set isApproved
     *
     * @param boolean $isApproved
     *
     * @return Proposal
     */
    public function setIsApproved($isApproved)
    {
        $this->isApproved = $isApproved;

        return $this;
    }

    /**
     * Get isApproved
     *
     * @return boolean
     */
    public function getIsApproved()
    {
        return $this->isApproved;
    }

    /**
     * Set isArchived
     *
     * @param boolean $isArchived
     *
     * @return Proposal
     */
    public function setIsArchived($isArchived)
    {
        $this->isArchived = $isArchived;

        return $this;
    }

    /**
     * Get isArchived
     *
     * @return boolean
     */
    public function getIsArchived()
    {
        return $this->isArchived;
    }

    /**
     * Set isCreating
     *
     * @param boolean $isCreating
     *
     * @return Proposal
     */
    public function setIsCreating($isCreating)
    {
        $this->isCreating = $isCreating;

        return $this;
    }

    /**
     * Get isCreating
     *
     * @return boolean
     */
    public function getIsCreating()
    {
        return $this->isCreating;
    }
}
